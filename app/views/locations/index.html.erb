<div id='map-two' class='map'> </div>


<script>
L.mapbox.accessToken = 'pk.eyJ1IjoiZGF2aWRqYXZhdGUiLCJhIjoiNHpuQVN6ZyJ9.pP7v8vVU8qXdQID-CZI99w';
var map = L.mapbox.map('map-two', 'davidjavate.jd8jfekm', {
    scrollWheelZoom: false
}).setView([37.7833,-122.4167], 14);

var myLayer = L.mapbox.featureLayer().addTo(map);

console.log(<%=raw @geojson %>)

myLayer.setGeoJSON(<%=raw @geojson %>);
myLayer.on('click', function(e) {
    e.layer.unbindPopup();
    window.location.href=e.layer.feature.properties.url;
});
</script>